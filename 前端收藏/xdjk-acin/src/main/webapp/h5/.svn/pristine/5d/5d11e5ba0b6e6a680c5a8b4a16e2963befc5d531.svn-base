<script>
    $(function(){
        var high=$('.dropdown').height();
        $('#u2').height(high);
        $('#u2_state0').height(high);

        var Request = new Object();
        Request  = GetRequest();
        var id = Request["id"];
        getMenu(id);
        function getMenu(id) {
            var params = {"id": id, "parent_id": "0"};
            $.ajax({
                type: "POST",
                dataType: "json",
                url: "/menu/getMenu",
                data: params,
                success: function (data) {
                    for (var i=0;i<data.length;i++)
                    {
                        $('.trueMenu').append('<div class="oneMenuA'+data[i].id+'"><a class="oneMenuA" id="'+data[i].id+'" onclick="comeTwoMenu(this)" style="cursor: pointer;" name="'+id+'" >'+data[i].menu_name+'</a></div>');
                    }
                    // var list = data;
                    // window.location.href = "index.jsp?pathlist=" + list;
                },
                error: function (e) {
                    alert("异常！");
                }
            });
        }

    })
    function GetRequest() {
        var url = location.search; //获取url中"?"符后的字串
        var theRequest = new Object();
        if (url.indexOf("?") != -1) {
            var str = url.substr(1);
            strs = str.split("&");
            for(var i = 0; i < strs.length; i ++) {
                theRequest[strs[i].split("=")[0]]=(strs[i].split("=")[1]);
            }
        }
        return theRequest;
    }
</script>

